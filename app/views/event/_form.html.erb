<%= form_for :event, url: event_index_path do |f| %>
  <p>
    <%= f.label :title %>
    <%= f.text_field :title %>
  </p>
  <p>
  	<%= f.label :community %>
  	<%= f.text_field :community %>
  </p>
  <p>
  	<%= f.label :address %>
  	<%= f.text_field :address %>
  </p>
  <p>
  	<%= f.label :organizer %>
  	<%= f.text_field :organizer %>
  </p>
  <p>
  	<%= f.label :date %>
  	<%= f.text_field :date %>
  </p>
  <p>
  	<%= f.label :start_time %>
  	<%= f.text_field :start_time %>
  </p>
  <p>
  	<%= f.label :end_time %>
  	<%= f.text_field :end_time %>
  </p>
  <p>
  	<%= f.submit %>
  </p>
<% end %>
